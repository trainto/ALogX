{
  "name": "Catalogger",
  "productName": "Catalogger",
  "description": "Android Log Viewer",
  "author": {
    "name": "trainto"
  },
  "license": "MIT",
  "version": "0.5.0",
  "main": "./app/main.js",
  "scripts": {
    "app": "./node_modules/electron/cli.js ./",
    "webpack": "./node_modules/webpack/bin/webpack.js ./entry.js bundle.js",
    "watch": "./node_modules/webpack/bin/webpack.js --progress --colors --watch",
    "pack": "webpack && build --dir",
    "dist": "webpack && build"
  },
  "build": {
    "appId": "me.yoursun.catalogger",
    "asar": false,
    "files": [
      "app/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "fixed-data-table-2": "^0.7.11",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-measure": "^1.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "css-loader": "^0.26.1",
    "electron": "^1.4.15",
    "electron-builder": "^13.11.1",
    "file-loader": "^0.10.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  }
}
